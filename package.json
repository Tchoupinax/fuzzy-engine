{
  "name": "fuzzy-engine",
  "version": "1.5.1",
  "description": "Docker registry UI made with love",
  "author": "Tchoupinax <corentinfiloche@hotmail.fr> (https://corentinfiloche.xyz)",
  "licence": "MIT",
  "keywords": [
    "docker",
    "docker-registry",
    "dockerui"
  ],
  "homepage": "https://fuzzy-engine.corentinfiloche.xyz",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tchoupinax/fuzzy-engine"
  },
  "bugs": {
    "url": "https://github.com/Tchoupinax/fuzzy-engine/issues"
  },
  "private": true,
  "scripts": {
    "watch": "bash -c 'source .env && nuxi dev'",
    "test": "bash -c 'source .env && vitest'",
    "build": "nuxi build",
    "docker:build": "docker buildx build --platform linux/amd64,linux/arm64 -t tchoupinax/fuzzy-engine . --push",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@aws-sdk/client-ecr": "^3.131.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "cookieparser": "^0.1.0",
    "express": "^4.18.1",
    "js-cookie": "^2.2.1",
    "mini-toastr": "^0.8.1",
    "nuxt-edge": "^2.16.0-27603381.123206cb",
    "pretty-bytes": "^5.6.0",
    "timeago.js": "^4.0.2",
    "vue-clipboard2": "^0.3.3",
    "vue-notifications": "^1.0.2"
  },
  "devDependencies": {
    "@nuxt/bridge": "npm:@nuxt/bridge-edge@^3.0.0-27566698.2b0045e",
    "@nuxtjs/eslint-config": "^4.0.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.0.0",
    "@nuxtjs/tailwindcss": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vue/test-utils": "^1.1.0",
    "autoprefixer": "^9.8.8",
    "babel-eslint": "^10.1.0",
    "defu": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-nuxt": ">=1.0.0",
    "nodemon": "^2.0.20",
    "postcss": "^7.0.39",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "vitest": "^0.19.0"
  }
}
